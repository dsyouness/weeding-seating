{
  "version": 2,
  "builds": [
    {
      "src": "client/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.(js|css|ico|png|jpg|jpeg|gif|svg)",
      "dest": "/client/frontend/dist/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/client/frontend/dist/index.html"
    }
  ],
  "functions": {
    "api/guests.js": {
      "includeFiles": "data/**"
    }
  }
}
